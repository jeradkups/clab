{
    "interfaces": {
        "rx-threads": 2,
        "tx-threads": 2,
        "links": [
            {
                "interface": "eth1",
                "rx-threads": 3,
                "tx-threads": 3
            }
        ],
        "network": [
            {
                "__comment__": "Network",
                "interface": "eth1",
                "address": "10.10.1.6",
                "gateway": "10.10.1.5"
            }
        ],
        "access": [
            {
                "__comment__": "DHCP Clients",
                "interface": "eth2",
                "network-interface": "eth1",
                "type": "ipoe",
                "vlan-mode": "1:1",
                "ipv6": false,
                "outer-vlan-min": 100,
                "outer-vlan-max": 110,
                "outer-vlan-step": 1,
                "inner-vlan-min": 0,
                "inner-vlan-max": 0,
                "inner-vlan-step": 0,
                "stream-group-id": 1
            },
	    {
                "__comment__": "Static Clients",
                "interface": "eth2",
                "network-interface": "eth1",
                "type": "ipoe",
                "vlan-mode": "1:1",
                "ipv6": false,
                "address": "99.0.0.100",
                "address-iter": "0.0.0.1",
                "gateway": "99.0.0.1",
                "outer-vlan-min": 100,
                "outer-vlan-max": 110,
                "outer-vlan-step": 1,
                "inner-vlan-min": 0,
                "inner-vlan-max": 0,
                "inner-vlan-step": 0,
                "stream-group-id": 2
	    }
        ]
    },
    "access-line": {
        "agent-remote-id": "BNG.DEMO.{session-global}",
        "agent-circuit-id": "BNG.DEMO:{session-global}"
    },
    "dhcp": {
        "enable": true,
        "broadcast": true,
        "access-line": true
    },
    "sessions": {
        "count": 10
    },
    "session-traffic": {
        "ipv4-pps": 10
    },
    "streams": [
        {
            "stream-group-id": 1,
            "name": "S1",
            "type": "ipv4",
            "direction": "both",
            "priority": 0,
            "length": 512,
            "pps": 10
        },
        {
            "stream-group-id": 2,
            "name": "S2",
            "type": "ipv4",
            "direction": "both",
            "priority": 0,
            "length": 512,
            "pps": 10
        }
    ]
}
