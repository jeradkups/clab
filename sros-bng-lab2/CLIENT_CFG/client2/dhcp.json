                "tx-threads": 3
            }
        ],
	"network": [
            {
                "__comment__": "Network",
		"interface": "eth2",
		"address": "10.10.1.6",
		"gateway": "10.10.1.5"
            }
	],
        "access": [
            {
                "__comment__": "DHCP Clients",
                "interface": "eth1",
